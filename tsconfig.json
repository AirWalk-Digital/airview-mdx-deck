{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages2/pads/[id].jss",
    "pages2/pads/[id].jss",
    ".next/types/**/*.ts",
    "app/page.tsxx",
    "app2/page.tsxsxx",
    "app/pad2/page.jsss",
    "app/pad2/page.jsss"
  ],
  "exclude": [
    "node_modules"
  ]
}